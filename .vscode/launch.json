  "version": "0.2.0",
  "configurations": [
  
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Launch Edge",
      "request": "launch",
      "type": "pwa-msedge",
      "url": "http://localhost:1234",
      "webRoot": "${workspaceFolder}"
    },
  {
    "name": "Attach to Edge",
    "port": 3000,
    "request": "attach",
    "type": "pwa-msedge",
    "url": "http://127.0.0.1:3000/",
    "webRoot": "${workspaceFolder}"
  },
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "type": "pwa-msedge",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:49239/",
      "webRoot": "${workspaceFolder}"
    }
  ]
}